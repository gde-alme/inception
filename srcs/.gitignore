# env
.env

# Node.js related
node_modules/       # Ignore dependencies directory
npm-debug.log       # Ignore npm debug log
yarn-error.log      # Ignore yarn error log
yarn-debug.log*     # Ignore all yarn debug logs
*.pnp.*             # Ignore Plug'n'Play artifacts
.pnp.js             # Ignore PnP resolution file

# Build directories
/dist               # Ignore built files
/build              # Ignore build directories
/public/assets      # Ignore compiled assets

# Environment configurations
.env                # Ignore environment config files
.env.local
.env.development.local
.env.test.local
.env.production.local

# Editor & OS meta-data
*.swp               # Vim swap files
*.swo               # Vim swap files
*.idea/             # Ignore IntelliJ project directory
.vscode/            # Visual Studio Code directory
.DS_Store           # Files generated by macOS
Thumbs.db           # Files generated by Windows

# Logs
logs/
*.log
*.log.*

# Runtime data
pids/
*.pid
*.seed
*.pid.lock

# Directories used by tools like jest, parcel, etc.
.cache/
coverage/           # Code coverage reports

# Miscellaneous
*.gz                # Ignore gzip files
*.tar               # Ignore tarball files
*.rar               # Ignore rar files
*.zip               # Ignore zip files
*.7z                # Ignore 7z compressed files
*.sln               # Ignore Visual Studio solution files
*.iml               # Ignore IntelliJ module files

